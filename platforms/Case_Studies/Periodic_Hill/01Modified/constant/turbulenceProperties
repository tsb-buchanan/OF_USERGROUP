FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      turbulenceProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

simulationType RAS;

RAS
{
    RASModel        modifiedmykOmegaSST; //mykOmegaSST
    turbulence      on;
    printCoeffs     on;


    useRST          1.0; 
    usekDeficit     1.0;
    rampStartTime   2000;
    rampEndTime     3000;

}

